  "keywords": "communication,alarm,housegard,origo,smoke,detector,gateway,hub,RF,433MHz,arduino,esp8266,esp32",
  "license": "Apache-2.0",
  "authors":
  {
    "name": "Fred Larsen",
    "email": "fredilarsen+github@hotmail.com",
    "maintainer": true
  },
  "repository":
  {
    "type": "git",
    "url": "https://github.com/fredilarsen/OrigoSmokeDetector.git"
  },
  "version": "4.0",
  "examples": [
    "examples/*/*.ino",
    "examples/*/*/*.ino",
    "examples/*/*/*/*.ino"
    ],
  "frameworks": "arduino",
  "dependencies": {
      "PJON" : ">=13.0",
